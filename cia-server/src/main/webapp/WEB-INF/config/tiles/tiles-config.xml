<!DOCTYPE tiles-definitions PUBLIC 
 	"-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"  
 	"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>


	<definition name="layout.basic2" template="/theme/basic2/include/layout.jsp">
		<put-attribute name="wrap_content" value="" />
	</definition>
	<definition name="basic2:*" extends="layout.basic2" >
		<put-attribute name="wrap_content" value="/theme/basic2/{1}.jsp"/>
	</definition>
	<definition name="basic2:*/*" extends="layout.basic2">
		<put-attribute name="wrap_content" value="/theme/basic2/{1}/{2}.jsp"/>
	</definition>
	<definition name="basic2:*/*/*" extends="layout.basic2">
		<put-attribute name="wrap_content" value="/theme/basic2/{1}/{2}/{3}.jsp"/>
	</definition>
	
	
	
	<definition name="layout.basic2_mobile" template="/theme/basic2_mobile/include/layout.jsp">
		<put-attribute name="wrap_content" value="" />
	</definition>
	<definition name="basic2_mobile:*" extends="layout.basic2_mobile" >
		<put-attribute name="wrap_content" value="/theme/basic2_mobile/{1}.jsp"/>
	</definition>
	<definition name="basic2_mobile:*/*" extends="layout.basic2_mobile">
		<put-attribute name="wrap_content" value="/theme/basic2_mobile/{1}/{2}.jsp"/>
	</definition>
	<definition name="basic2_mobile:*/*/*" extends="layout.basic2_mobile">
		<put-attribute name="wrap_content" value="/theme/basic2_mobile/{1}/{2}/{3}.jsp"/>
	</definition>
	
	
	
	<definition name="layout.basic" template="/theme/basic/include/layout.jsp">
		<put-attribute name="wrap_content" value="" />
	</definition>
	
	<definition name="basic:*" extends="layout.basic" >
		<put-attribute name="wrap_content" value="/theme/basic/{1}.jsp"/>
	</definition>
	<definition name="basic:*/*" extends="layout.basic">
		<put-attribute name="wrap_content" value="/theme/basic/{1}/{2}.jsp"/>
	</definition>
	<definition name="basic:*/*/*" extends="layout.basic">
		<put-attribute name="wrap_content" value="/theme/basic/{1}/{2}/{3}.jsp"/>
	</definition>
	
	
	
	

	<definition name="layout.run" template="/theme/admin/include/layout_run.jsp">
		<put-attribute name="wrap_content" value="" />
	</definition>
	 
	<definition name="run:*" extends="layout.run">
		<put-attribute name="wrap_content" value="/theme/admin/{1}.jsp" />
	</definition>
	<definition name="run:*/*" extends="layout.run">
		<put-attribute name="wrap_content" value="/theme/admin/{1}/{2}.jsp" />
	</definition>
	<definition name="run:*/*/*" extends="layout.run">
		<put-attribute name="wrap_content" value="/theme/admin/{1}/{2}/{3}.jsp" />
	</definition>
	
		

</tiles-definitions>